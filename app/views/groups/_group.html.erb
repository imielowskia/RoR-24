<div id="<%= dom_id group %>">


  <div class="row">
    <hr>
    <div class="col"><%= group.nr %></div>
    <div class="col"><%= group.nazwa %></div>
    <div class="col"><%= group.field.nazwa %></div>
    <div class="col">
      <% group.students.each do |student| %>
        <%= student.imie %> <%= student.nazwisko %><br>
      <% end %>
    </div>
    <div class="col"><%= link_to "Show this group", group, class: "btn btn-sm btn-primary" %></div>
    <hr>
  </div>


  <% group.courses.each do |course| %>
    <div class="row">
      <div class="col"> <%= course.nazwa %> </div>
      <div class="col"> <%= link_to "Oceny czÄ…stkowe", detailgrade_group_path(group.id, course.id ), class: "btn btn-sm btn-primary" %> </div>
      <hr>
    </div>
  <% end %>
  </div>


</div>
