<div id="<%= dom_id group %>">
  <p>
    <strong>Nr:</strong>
    <%= group.nr %>
  </p>

  <p>
    <strong>Nazwa:</strong>
    <%= group.nazwa %>
  </p>

  <p>
    <strong>Lista studentów:</strong><br>
    <% group.students.each do |student| %>
      <%= student.imie %> <%= student.nazwisko %><br>
    <% end %>
  </p>

  <p>
    <strong>Lista przedmiotów:</strong><br>
    <% group.courses.each do |course| %>
      <%= course.nazwa %> <br>
    <% end %>
  </p>

</div>
